@model GerarHorarioTecnicos

@{
    ViewData["Title"] = "Gerar Horário Técnicos";
}

<div class="row">
    <form asp-action="GerarHorarioTecnicos">
        <div class="form-group">
            <label asp-for="DataInicioSemana" class="control-label">Data Início da Semana</label>
            <input asp-for="DataInicioSemana " class="form-control" />
        </div>
        <div class="form-group">
            <input type="submit" value="Gerar" class="btn btn-default" />
        </div>
    </form>
</div>


